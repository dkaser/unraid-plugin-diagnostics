<!--
The 'install' script.
-->
<FILE Run="/bin/bash">
<INLINE>
<![CDATA[
upgradepkg --install-new /boot/config/plugins/plugin-diagnostics/unraid-plugin-diagnostics-2025.10.21.0220-noarch-1.txz

# cleanup old versions
rm -f $(ls /boot/config/plugins/plugin-diagnostics/unraid-plugin-diagnostics-*.txz 2>/dev/null | grep -v '2025.10.21.0220')

echo ""
echo "----------------------------------------------------"
echo " plugin-diagnostics has been installed."
echo " Version: 2025.10.21.0220"
echo "----------------------------------------------------"
echo ""
]]>
</INLINE>
</FILE>

<!--
The 'remove' script.
-->
<FILE Run="/bin/bash" Method="remove">
<INLINE>
<![CDATA[
removepkg unraid-plugin-diagnostics

